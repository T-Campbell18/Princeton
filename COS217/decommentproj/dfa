Normal (accept)
      /: SLASH
      ": STRING
      ': CHAR	
      other: NORMAL

SLASH (accept)
      *: COMMENT
      /: SLASH
      ": STRING
      ': CHAR
      other: NORMAL

STRING (accept)
      ": NORMAL
      \: STRINGESCAPE
      other: STRING

STRINGESCAPE (accept)
      other: STRING

CHAR (accept)
      ': NORMAL
      \: CHARESCAPE
      other: CHAR

CHARESCAPE (accept)
      other: CHAR
      
COMMENT (reject)
      *: STAR
      other: COMMENT
      
STAR (reject)
      /: NORMAL
      *: STAR
      other: COMMENT